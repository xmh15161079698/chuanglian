@{
    ViewBag.Title = "代码模板";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="kr-form-wrap" id="form">
    <div class="col-xs-6 kr-form-item"  data-table="KR_Base_CodeSchema" >
        <div class="kr-form-item-title">模板名称<font face="宋体">*</font></div>
        <input id="F_Name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 kr-form-item"  data-table="KR_Base_CodeSchema" >
        <div class="kr-form-item-title">模板分类</div>
        <div id="F_Catalog"  ></div>
    </div>
    <div class="col-xs-12 kr-form-item"  data-table="KR_Base_CodeSchema" >
        <div class="kr-form-item-title">模板描述</div>
        <textarea id="F_Description" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/KR_CodeGeneratorModule/Views/CodeSchema/Form.js")

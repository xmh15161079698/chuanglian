@{
    ViewBag.Title = "地址管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="kr-form-wrap" id="form">
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">用户编号<font face="宋体">*</font></div>
        <input id="F_User_Id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">所在地区<font face="宋体">*</font></div>
        <input id="F_Area" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">详细地址<font face="宋体">*</font></div>
        <input id="F_Detail_Address" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">联系人<font face="宋体">*</font></div>
        <input id="F_ReceiveName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">联系人电话<font face="宋体">*</font></div>
        <input id="F_ReceivePhone" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 kr-form-item" data-table="WD_Address">
        <div class="kr-form-item-title">默认地址<font face="宋体">*</font></div>
        <input id="F_Is_Default" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/KR_CodeModule/Views/WD_Address/Form.js")
